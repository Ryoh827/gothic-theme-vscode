      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#C199FF"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FF948A"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FC9700"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#C199FF"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
