    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#F2F2F2"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#A7FF9D"
      }
    },
    {
      "scope": [
        "markup.bold.markdown",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#A7FF9D"
