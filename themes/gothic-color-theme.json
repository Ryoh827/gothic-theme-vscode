    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E599FF"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FF948A"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FC9700"
      }
    },
    {
