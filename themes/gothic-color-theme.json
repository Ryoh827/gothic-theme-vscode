      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#00E3E3"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#9A9A9A"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#7A97FF"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#00CC00"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#BEBEBE"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FF948A"
      }
