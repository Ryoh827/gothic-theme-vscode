      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#BEBEBE"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#00CC00"
      }
    },
    {
      "scope": [
        "markup.bold.markdown",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#00CC00"
      }
    },
    {
      "scope": "markup.quote.markdown meta.paragraph.markdown",
      "settings": {
        "foreground": "#9A9A9A",
